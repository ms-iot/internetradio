    Loaded="Page_Loaded"
    Unloaded="Page_Unloaded">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <MediaElement Name="radioPlayer" Volume="0.1"></MediaElement>
    </Grid>
</Page>
